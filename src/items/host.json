{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Exception"
      }
    }
  },
  "aggregator": {
    "batchSize": 100,
    "flushTimeout": "00:00:05"
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "functionTimeout": "00:00:30",
  "extensions": {
    "http": {
      "routePrefix": ""
    }
  }
}
